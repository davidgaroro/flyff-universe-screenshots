{
  "manifest_version": 3,
  "name": "Flyff Universe - Screenshots",
  "version": "1.0.0",
  "default_locale": "en",
  "description": "__MSG_extDescription__",
  "action": {},
  "background": {
    "scripts": ["background.firefox.js"]
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+F"
      }
    }
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "permissions": ["activeTab", "downloads", "storage"]
}
